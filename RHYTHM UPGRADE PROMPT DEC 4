0.2 URL Hash Routing
File:Â src/App.tsxÂ + newÂ src/hooks/useHashRouter.ts
// Simple hash-based routing without React Router
// Preserves existing tab-based navigation but adds URL support

const useHashRouter = () => {
  const [hash, setHash] = useState(window.location.hash);

  useEffect(() => {
    const handler = () => setHash(window.location.hash);
    window.addEventListener('hashchange', handler);
    return () => window.removeEventListener('hashchange', handler);
  }, []);

  const navigate = (path: string) => {
    window.location.hash = path;
  };

  return { hash, navigate };
};

// URL patterns:
// #/feed - FYP feed
// #/producer/[id] - Producer profile
// #/track/[id] - Track detail (for sharing)
// #/messages - DM inbox
// #/settings - Settings (existing)
Integration with existing routing:
* CurrentÂ activeTabÂ state syncs with hash
* Existing navigation continues to work
* URLs become shareable
* Browser back button works
0.3 Lazy Context Loading
File:Â src/App.tsxÂ + new wrapper components
// Instead of wrapping everything in all providers at once:
// OLD: <SocialProvider><FeedProvider><MessagingProvider>...

// NEW: Load contexts on-demand
const LazyFeedProvider = lazy(() => import('./contexts/FeedContext'));
const LazySocialProvider = lazy(() => import('./contexts/SocialContext'));

// Only load when accessing those features
{activeTab === 'feed' && (
  <Suspense fallback={<FeedSkeleton />}>
    <LazyFeedProvider>
      <FeedView />
    </LazyFeedProvider>
  </Suspense>
)}

Phase 1: Database Schema Additions
New Tables (PostgreSQL)
-- Producer profiles (extends users)
CREATE TABLE producer_profiles (
  id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
  user_id UUID UNIQUE REFERENCES users(id) ON DELETE CASCADE,
  display_name VARCHAR(100),
  bio TEXT,
  avatar_url TEXT,
  cover_image_url TEXT,
  website_url TEXT,
  instagram_handle VARCHAR(50),
  twitter_handle VARCHAR(50),
  verified BOOLEAN DEFAULT false,
  producer_tier VARCHAR(20) DEFAULT 'starter', -- starter, pro, elite
  total_sales INTEGER DEFAULT 0,
  total_earnings DECIMAL(12,2) DEFAULT 0,
  payout_address TEXT, -- crypto wallet or PayPal
  created_at TIMESTAMPTZ DEFAULT NOW(),
  updated_at TIMESTAMPTZ DEFAULT NOW()
);

-- Social follows
CREATE TABLE follows (
  id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
  follower_id UUID REFERENCES users(id) ON DELETE CASCADE,
  following_id UUID REFERENCES users(id) ON DELETE CASCADE,
  created_at TIMESTAMPTZ DEFAULT NOW(),
  UNIQUE(follower_id, following_id)
);

-- Track engagement (extends existing tracks)
CREATE TABLE track_engagement (
  id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
  track_id UUID REFERENCES tracks(id) ON DELETE CASCADE,
  play_count INTEGER DEFAULT 0,
  like_count INTEGER DEFAULT 0,
  share_count INTEGER DEFAULT 0,
  download_count INTEGER DEFAULT 0,
  trending_score DECIMAL(10,4) DEFAULT 0,
  last_trending_calc TIMESTAMPTZ,
  UNIQUE(track_id)
);

-- Likes
CREATE TABLE likes (
  id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
  user_id UUID REFERENCES users(id) ON DELETE CASCADE,
  track_id UUID REFERENCES tracks(id) ON DELETE CASCADE,
  created_at TIMESTAMPTZ DEFAULT NOW(),
  UNIQUE(user_id, track_id)
);

-- Comments
CREATE TABLE comments (
  id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
  user_id UUID REFERENCES users(id) ON DELETE CASCADE,
  track_id UUID REFERENCES tracks(id) ON DELETE CASCADE,
  parent_id UUID REFERENCES comments(id) ON DELETE CASCADE, -- for replies
  content TEXT NOT NULL,
  created_at TIMESTAMPTZ DEFAULT NOW(),
  updated_at TIMESTAMPTZ DEFAULT NOW()
);

-- Direct messages
CREATE TABLE conversations (
  id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
  created_at TIMESTAMPTZ DEFAULT NOW(),
  updated_at TIMESTAMPTZ DEFAULT NOW()
);

CREATE TABLE conversation_participants (
  conversation_id UUID REFERENCES conversations(id) ON DELETE CASCADE,
  user_id UUID REFERENCES users(id) ON DELETE CASCADE,
  last_read_at TIMESTAMPTZ,
  PRIMARY KEY(conversation_id, user_id)
);

CREATE TABLE messages (
  id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
  conversation_id UUID REFERENCES conversations(id) ON DELETE CASCADE,
  sender_id UUID REFERENCES users(id) ON DELETE CASCADE,
  content TEXT NOT NULL,
  created_at TIMESTAMPTZ DEFAULT NOW()
);

-- Beat packs
CREATE TABLE beat_packs (
  id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
  producer_id UUID REFERENCES users(id) ON DELETE CASCADE,
  title VARCHAR(200) NOT NULL,
  description TEXT,
  cover_image_url TEXT,
  price DECIMAL(10,2) NOT NULL,
  sale_price DECIMAL(10,2), -- optional discount
  is_published BOOLEAN DEFAULT false,
  created_at TIMESTAMPTZ DEFAULT NOW(),
  updated_at TIMESTAMPTZ DEFAULT NOW()
);

CREATE TABLE beat_pack_tracks (
  pack_id UUID REFERENCES beat_packs(id) ON DELETE CASCADE,
  track_id UUID REFERENCES tracks(id) ON DELETE CASCADE,
  position INTEGER NOT NULL,
  PRIMARY KEY(pack_id, track_id)
);

-- Licensing tiers
CREATE TABLE license_tiers (
  id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
  track_id UUID REFERENCES tracks(id) ON DELETE CASCADE,
  tier_name VARCHAR(50) NOT NULL, -- basic, premium, exclusive
  price DECIMAL(10,2) NOT NULL,
  streams_limit INTEGER, -- null = unlimited
  distribution_rights TEXT[],
  pdf_template_key TEXT, -- S3 key for license PDF template
  created_at TIMESTAMPTZ DEFAULT NOW()
);

-- Purchases
CREATE TABLE purchases (
  id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
  buyer_id UUID REFERENCES users(id) ON DELETE CASCADE,
  seller_id UUID REFERENCES users(id),
  track_id UUID REFERENCES tracks(id) ON DELETE SET NULL,
  pack_id UUID REFERENCES beat_packs(id) ON DELETE SET NULL,
  license_tier_id UUID REFERENCES license_tiers(id),
  amount DECIMAL(10,2) NOT NULL,
  platform_fee DECIMAL(10,2) NOT NULL,
  seller_payout DECIMAL(10,2) NOT NULL,
  payment_intent_id TEXT, -- Stripe
  status VARCHAR(20) DEFAULT 'pending', -- pending, completed, refunded
  license_pdf_url TEXT,
  created_at TIMESTAMPTZ DEFAULT NOW()
);

-- Payout records (blockchain transparency)
CREATE TABLE payouts (
  id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
  producer_id UUID REFERENCES users(id) ON DELETE CASCADE,
  amount DECIMAL(12,2) NOT NULL,
  method VARCHAR(20) NOT NULL, -- paypal, crypto, bank
  transaction_hash TEXT, -- blockchain tx hash if crypto
  status VARCHAR(20) DEFAULT 'pending',
  created_at TIMESTAMPTZ DEFAULT NOW(),
  completed_at TIMESTAMPTZ
);

-- Feed algorithm data
CREATE TABLE feed_interactions (
  id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
  user_id UUID REFERENCES users(id) ON DELETE CASCADE,
  track_id UUID REFERENCES tracks(id) ON DELETE CASCADE,
  interaction_type VARCHAR(20) NOT NULL, -- view, like, share, purchase, skip
  duration_seconds INTEGER, -- how long they listened
  created_at TIMESTAMPTZ DEFAULT NOW()
);

-- Track visibility settings (add to existing tracks table)
-- ALTER TABLE tracks ADD COLUMN visibility VARCHAR(20) DEFAULT 'private'; -- private, public, unlisted
-- ALTER TABLE tracks ADD COLUMN is_for_sale BOOLEAN DEFAULT false;
-- ALTER TABLE tracks ADD COLUMN preview_file_key TEXT; -- watermarked preview
-- ALTER TABLE tracks ADD COLUMN genre VARCHAR(50);
-- ALTER TABLE tracks ADD COLUMN tags TEXT[];
Indexes for Performance
CREATE INDEX idx_follows_follower ON follows(follower_id);
CREATE INDEX idx_follows_following ON follows(following_id);
CREATE INDEX idx_likes_track ON likes(track_id);
CREATE INDEX idx_likes_user ON likes(user_id);
CREATE INDEX idx_comments_track ON comments(track_id);
CREATE INDEX idx_track_engagement_trending ON track_engagement(trending_score DESC);
CREATE INDEX idx_feed_interactions_user ON feed_interactions(user_id, created_at DESC);
CREATE INDEX idx_tracks_visibility ON tracks(visibility) WHERE visibility = 'public';
CREATE INDEX idx_tracks_genre ON tracks(genre) WHERE visibility = 'public';
CREATE INDEX idx_messages_conversation ON messages(conversation_id, created_at DESC);

Phase 2: API Endpoints
File:Â api/index.jsÂ (additions)
Producer Profile Endpoints
GET    /api/producers/:id           - Get producer profile
PUT    /api/producers/me            - Update own profile
GET    /api/producers/:id/tracks    - Get producer's public tracks
GET    /api/producers/:id/stats     - Get producer analytics
Social Endpoints
POST   /api/follow/:userId          - Follow a producer
DELETE /api/follow/:userId          - Unfollow
GET    /api/followers/:userId       - Get followers list
GET    /api/following/:userId       - Get following list
POST   /api/tracks/:id/like         - Like a track
DELETE /api/tracks/:id/like         - Unlike
GET    /api/tracks/:id/comments     - Get comments
POST   /api/tracks/:id/comments     - Add comment
DELETE /api/comments/:id            - Delete own comment
Feed Endpoints
GET    /api/feed                    - Get personalized FYP feed
GET    /api/feed/trending           - Get trending tracks
GET    /api/feed/new                - Get newest releases
GET    /api/feed/following          - Get from followed producers
POST   /api/feed/interaction        - Log feed interaction
Marketplace Endpoints
GET    /api/marketplace/browse      - Browse beats (filters: genre, bpm, key, price)
GET    /api/marketplace/packs       - Browse beat packs
GET    /api/packs/:id               - Get pack details
POST   /api/packs                   - Create beat pack
PUT    /api/packs/:id               - Update pack
DELETE /api/packs/:id               - Delete pack
Licensing Endpoints
GET    /api/tracks/:id/licenses     - Get license tiers
POST   /api/tracks/:id/licenses     - Create license tier
PUT    /api/licenses/:id            - Update license
POST   /api/purchase/track/:id      - Purchase track license
POST   /api/purchase/pack/:id       - Purchase beat pack
GET    /api/purchases               - Get user's purchases
GET    /api/sales                   - Get producer's sales
Messaging Endpoints
GET    /api/conversations           - Get conversations
GET    /api/conversations/:id       - Get messages
POST   /api/conversations           - Start conversation
POST   /api/conversations/:id/messages - Send message
Payout Endpoints
GET    /api/payouts                 - Get payout history
POST   /api/payouts/request         - Request payout
GET    /api/payouts/balance         - Get available balance

Phase 3: Feed Algorithm
Trending Score Calculation
// Run every 15 minutes via cron job
function calculateTrendingScore(track) {
  const now = Date.now();
  const ageHours = (now - track.createdAt) / (1000 * 60 * 60);

  // Decay factor - newer content scores higher
  const decay = Math.pow(0.95, ageHours / 24);

  // Engagement weights
  const engagementScore =
    (track.playCount * 1) +
    (track.likeCount * 5) +
    (track.shareCount * 10) +
    (track.purchaseCount * 50) +
    (track.commentCount * 3);

  // Velocity bonus - recent engagement matters more
  const recentEngagement = getEngagementLast24Hours(track.id);
  const velocityBonus = recentEngagement * 2;

  return (engagementScore + velocityBonus) * decay;
}
FYP Feed Assembly
async function getFYPFeed(userId, page = 1, limit = 20) {
  // Get user preferences from interaction history
  const preferences = await getUserPreferences(userId);

  // Mix: 40% trending, 30% from followed, 20% discovery, 10% new
  const trending = await getTrendingTracks(limit * 0.4, preferences);
  const following = await getFollowingTracks(userId, limit * 0.3);
  const discovery = await getDiscoveryTracks(userId, limit * 0.2, preferences);
  const newReleases = await getNewTracks(limit * 0.1);

  // Shuffle and dedupe
  const feed = shuffleWithPriority([
    ...trending.map(t => ({ ...t, source: 'trending' })),
    ...following.map(t => ({ ...t, source: 'following' })),
    ...discovery.map(t => ({ ...t, source: 'discovery' })),
    ...newReleases.map(t => ({ ...t, source: 'new' }))
  ]);

  return feed;
}

